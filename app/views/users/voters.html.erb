<section class="row">
  <div class="desc">
    <h2>Voters</h2>
  </div>
  <% if @voters.size > 0 %>
    <ul>
      <% @voters.each do |voter| %>
        <li><%= "#{voter.last_name}, #{voter.first_name} | #{voter.gender}" %></li>
        <i class="icon-female"></i>
      <% end %>
    </ul>
  <% else %>
    <p>
      There are no voters.
    </p>
  <% end %>
</section>
